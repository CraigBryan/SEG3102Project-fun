<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<% if admin? %>
	  <%= link_to "Create New Profile", admins_new_user_by_admin_path %>
<% end %>

<% @users = User.all %>
<% @users.each do |user| %>
    <%= user.email %><br/>
    <%= user.first_name %><br/>
    <%= user.last_name %><br/>
    <%= user.max_rent %><br/>
<% end %>