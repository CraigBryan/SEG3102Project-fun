<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1> Properties </h1>

    <% if @properties.size == 0 %>
      <p>No properties found</p>
    <% else %>
      <% @properties.each do |prop| %>
        <p>
          <%= prop.id %>
          <%= prop.prop_type %>
          <%= prop.address %>
          <%= prop.location %>
          <%= prop.number_bathrooms %>
          <%= prop.number_bedrooms %>
          <%= prop.rent_price %>
        </p>
      <% end %>
    <% end %>
  </div>
</div>
